<!DOCTYPE html>
<html>

<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>Dashboard-Distributor</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" type="text/css" media="screen" href="/stylesheets/main.css" />
    <script src="/js/main.js"></script>
    <!-- Compiled and minified CSS -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.100.2/css/materialize.min.css">

    <!-- Compiled and minified JavaScript -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.100.2/js/materialize.min.js"></script>
    <link rel="icon" type="image/png" href="/images/Logo2.png" />
    <link href="https://fonts.googleapis.com/css?family=Antic|Economica|Quicksand|Asap" rel="stylesheet">
</head>

<body onload="startTime() || checkday() || checktitle() ">
    <!-- Navigation bar -->
    <div class="nav">
        <span id="NavLogo"></span>
        <span id="NavHelp">?</span>
        <div class="chip">
            {{sessionname}}
        </div>
        <span id="NavSignup">
            <a href="/logout">Logout</a>
        </span>
    </div>
    <!--Header-->
    <div class="header">
        <div id="headerimage"></div>
        <div class="headertitle">
            <span id="Daytimefinder"></span>
            <span id="Sessionname">{{sessionname}}</span>
        </div>
    </div>
    <!--Dashboard-->
    <div id="wrapdashboard">
        <a href="/distributor/distributorprofile">
            <div class="myprofile">
                <div class="profileimage"></div>
                <span id="myprofiletext">My Profile</span>
            </div>
        </a>
        <a href="/distributor/top10">
            <div class="top10">
                <div class="top10image"></div>
                <span id="top10text">Top 10</span>
            </div>
        </a>

        <a href="/distributor/distributorcontent">
            <div class="mycontent">
                <div class="mycontentimage"></div>
                <span id="mycontenttext">My Movies</span>
            </div>
        </a>

        <a href="/distributor/distributoruploadmovie">
            <div class="uploadcontent">
                <div class="uploadcontentimage"></div>
                <span id="uploadcontenttext">Upload Movie</span>
            </div>
        </a>
    </div>


    <!-- Footer -->
    <div class="footer">
        <span id="FootHome"></span>
        <span id="FootTime"></span>
    </div>
</body>

</html>